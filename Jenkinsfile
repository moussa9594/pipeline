
pipeline {
    agent any

    stages {

        stage('Build'){
            steps {
                echo 'Building Pipeline ...'
            }
        }

        stage('Test'){
                    steps {
                        echo 'Testing Pipeline ...'
                    }
                }

        stage('Deploy'){
                    steps {
                        echo 'Deploying Pipeline ...'
                    }
                }
    }


}
